{
  "schemaVersion": 1,

  "id": "ranched",
  "version": "${version}",
  "license": [
    "MIT (Code)",
    "All Rights Reserved (Assets)"
  ],

  "name": "Ranched",
  "icon": "assets/ranched/icon.png",
  "description": "A mod that encourages better care of your animals!",

  "authors": [
    "Gaetano Russo (Ninni)",
    "Daniel Orr (Andante)"
  ],
  "contact": {
    "homepage": "https://moddingplayground.net/ranched",
    "sources": "https://github.com/moddingplayground/ranched-fabric",
    "issues": "https://github.com/moddingplayground/ranched-fabric/issues"
  },

  "environment": "*",
  "entrypoints": {
    "main": [
      "net.moddingplayground.ranched.impl.RanchedImpl",
      "net.moddingplayground.ranched.impl.block.RanchedBlocksImpl"
    ],
    "client": [
      "net.moddingplayground.ranched.impl.client.RanchedClientImpl",
      "net.moddingplayground.ranched.impl.client.block.RanchedBlocksClientImpl"
    ],

    "toymaker": [ "net.moddingplayground.ranched.impl.data.RanchedToymakerImpl" ]
  },
  "mixins": [ "ranched.mixins.json" ],

  "custom": {
    "modmenu": { "links": { "mod.ranched.contact.discord": "https://discord.moddingplayground.net" } }
  },

  "depends": {
    "java": ">=17",
    "minecraft": ">=1.18.2",
    "fabricloader": ">=0.13",
    "fabric": ">=0.48",
    "frame": ">=0.16"
  }
}
